 @prefix : <https://vascomap.com/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix ns: <https://vascomap.com/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prop: <https://vascomap.com/ontology/property/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

prop:code a owl:DatatypeProperty ;
    rdfs:label "code" ;
    rdfs:domain ns:Municipality .

prop:has a owl:ObjectProperty ;
    rdfs:label "has" ;
    rdfs:domain ns:Province ;
    rdfs:range ns:Municipality .

prop:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    rdfs:domain ns:Observation .

prop:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain ns:Municipality,
        ns:Province .

prop:observes a owl:ObjectProperty ;
    rdfs:label "observes" ;
    rdfs:domain ns:Observation ;
    rdfs:range ns:Data .

prop:takesPlaceIn a owl:ObjectProperty ;
    rdfs:label "takes place in" ;
    rdfs:domain ns:Observation ;
    rdfs:range ns:Province .

prop:value a owl:DatatypeProperty ;
    rdfs:label "value" ;
    rdfs:domain ns:Data .

ns:Data a owl:Class ;
    rdfs:label "Data" .

ns:Municipality a owl:Class ;
    rdfs:label "Municipality" .

ns:Observation a owl:Class ;
    rdfs:label "Observation" .

ns:Province a owl:Class ;
    rdfs:label "Province" .

